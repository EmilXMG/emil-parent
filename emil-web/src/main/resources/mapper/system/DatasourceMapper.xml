<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wf.ew.system.mapper.DatasourceMapper">


    <select id="listFull" resultType="com.wf.ew.system.entity.Datasource">
        SELECT *
        FROM datasource
    </select>

    <select id="getDsNameAndId" resultType="com.wf.ew.system.entity.Datasource">
        SELECT ds_id,ds_name
        FROM datasource
    </select>

</mapper>
